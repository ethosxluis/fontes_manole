USER FUNCTION AbrirMOD(cModFUN, cModLOC, cModARQ, cCONTRATO, cSEQUENCIA, cFORNECE, cPRODUTO, cAUTOR)


LOCAL aDados 		:= {}
Local cPath			:= alltrim(cModLOC)
Local cDot			:= alltrim(cModARQ)
Local cQuery		:= ""
Local hWord 		:= ""

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณProcesso para Criar a pasta Temp do Usuario e copiar o arquivo para a maquina do usuario para poder executar o Wordณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

cPASTA := "c:\temp"
MAKEDIR(cPASTA)

cCAMINHO := "c:\temp\"
 __CopyFile( cPath+cDot , cCAMINHO+cDot )

hWord 	:= OLE_CreateLink("TMSOLEWORD97")

OLE_SetProperty(hWord,oleWdWindowState,"MAX" )
OLE_SetProperty(hWord,oleWdVisible,.F.)
Ole_NewFile(hWord,"c:\temp\" + cDot)

cQuery := " SELECT * FROM "+RetSqlName("AH1")+", "+RetSqlName("SA2")+", "+RetSqlName("SB1")+", "+RetSqlName("UA2")+", "+RetSqlName("UA7")+", "+RetSqlName("CNL")
cQuery += "  WHERE AH1_FILIAL = '"+xFilial("AH1")+"' "
cQuery += "    AND AH1_CONTRA = '"+cCONTRATO+"' "
cQuery += "    AND AH1_SEQCON = '"+cSEQUENCIA+"' "
cQuery += "    AND "+RetSqlName("AH1")+".D_E_L_E_T_ = ' ' "
cQuery += "    AND A2_FILIAL = '"+xFilial("SA2")+"' "
cQuery += "    AND A2_COD = AH1_FORNEC "
cQuery += "    AND A2_LOJA = AH1_LOJAFO "
cQuery += "    AND "+RetSqlName("SA2")+".D_E_L_E_T_ = ' ' "
cQuery += "    AND B1_FILIAL = '"+xFilial("SB1")+"' "
cQuery += "    AND B1_COD = AH1_PRODUT "
cQuery += "    AND "+RetSqlName("SB1")+".D_E_L_E_T_ = ' ' "
cQuery += "    AND UA2_FILIAL = '"+xFilial("UA2")+"' "
cQuery += "    AND UA2_CONTRA = AH1_CONTRA "
cQuery += "    AND UA2_SEQCON = AH1_SEQCON "
cQuery += "    AND "+RetSqlName("UA2")+".D_E_L_E_T_ = ' ' "
cQuery += "    AND UA7_FILIAL = '"+xFilial("UA7")+"' "
cQuery += "    AND UA7_CODIGO =  UA2_CODCTR "
cQuery += "    AND "+RetSqlName("UA7")+".D_E_L_E_T_ = ' ' "
cQuery += "    AND CNL_FILIAL = '"+xFilial("CNL")+"' "
cQuery += "    AND CNL_CODIGO =  UA2_TIPOCO "
cQuery += "    AND "+RetSqlName("CNL")+".D_E_L_E_T_ = ' ' "

cQuery:= ChangeQuery(cQuery)
DbUseArea(.T.,"TOPCONN",TCGENQRY(,,cQuery),"TMPWORD",.F.,.F.)
DbSelectArea("TMPWORD")
TMPWORD->(DbGoTop())

cEXECUTA := cModFUN+"('"+hWord+"','"+cModLOC+"','"+cModARQ+"','"+cCONTRATO+"','"+cSEQUENCIA+"','"+cFORNECE+"','"+cPRODUTO+"','"+cAUTOR+"')"
&cEXECUTA

TMPWORD->(dbCloseArea())

OLE_UpdateFields(hWord)

cARQUIVO := ALLTRIM(cCONTRATO)+"_"+ALLTRIM(cSEQUENCIA)+"_"+ALLTRIM(cFORNECE)+"_"+ALLTRIM(cPRODUTO)+"_"+ALLTRIM(cAUTOR)+".dot"

If File (cCAMINHO+cARQUIVO)
	FErase(cCAMINHO+cARQUIVO)
EndIf

Ole_SaveAsFile(hWord, cCAMINHO+cARQUIVO)

OLE_CloseFile( hWord )
OLE_CloseLink( hWord )

cDESTINO := "\dirdoc\contratos\"
CPYT2S(cCAMINHO+cARQUIVO,cDESTINO,.T.)

ShellExecute( "Open", cARQUIVO, " ", cCAMINHO, 3 )

If File (cCAMINHO+cDot)
	FErase(cCAMINHO+cDot)
EndIf

return()

