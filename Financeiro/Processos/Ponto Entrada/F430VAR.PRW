USER FUNCTION F430VAR()
PRIVATE AVALIX := aClone(ParamIxb)
PRIVATE DDATE := CTOD("  /  /    ") 
PUBLIC ParamIxb
PUBLIC CDATE := SUBSTR(AVALIX[1][16],266,8)

IF ALLTRIM(DTOS(AVALIX[1][2])) == ""
	DDATE := CTOD(SUBSTR(CDATE,7,2)+"/"+SUBSTR(CDATE,5,2)+"/"+SUBSTR(CDATE,1,4))
	AVALIX[1][2] := DDATE 
	ParamIxb := aClone(AVALIX) 
ENDIF

RETURN()
