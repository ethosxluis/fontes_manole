#INCLUDE "PROTHEUS.CH"
USER FUNCTION VTEXTST
lOCAL LENTRA := .T.
SC5->(DBSETORDER(1))
SC6->(DBSETORDER(12))
SC5->(DBGOTOP())
WHILE SC5->(!EOF())
	LENTRA := .T.
	IF !EMPTY(SC5->C5_VTEX)
		SC6->(DBSEEK(xfilial("SC6")+SC5->C5_VTEX))
		WHILE SC5->C5_VTEX == SC6->C6_VTEX
			IF !EMPTY(SC6->C6_NOTA)
				LENTRA := .F.
			ENDIF
			SC6->(DBSKIP())
		END
		IF LENTRA
			u_AltPed(alltrim(SC5->C5_VTEX))
		ENDIF
	ENDIF
	SC5->(DBSKIP())
END
RETURN()