#INCLUDE "RWMAKE.CH"
#INCLUDE "TOPCONN.CH"

//FUNÇÃO - CD010CONT - PONTO DE ENTRADA NA INCLUSÃO DE CONTRATO DE DIREITO AUTORAL
//CD010ACONT

USER FUNCTION CD010CONT()
LOCAL CPERIOD := AH1->AH1_PERIOD
LOCAL DDTAULTP := CTOD("  /  /    ")

DDTAULTP := U_GCDAH1A(CPERIOD)

IF EMPTY(AH1->AH1_DTULTP) .OR. AH1->AH1_DTULTP <> DDTAULTP
	RECLOCK("AH1",.F.)
	AH1->AH1_DTULTP := DDTAULTP
	AH1->(MSUNLOCK())
ENDIF


RETURN()


