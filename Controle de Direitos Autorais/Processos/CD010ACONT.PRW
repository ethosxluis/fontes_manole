#INCLUDE "RWMAKE.CH"
#INCLUDE "TOPCONN.CH"

//FUNÇÃO - CD010ACONT - PONTO DE ENTRADA NA ALTERAÇÃO DE CONTRATO DE DIREITO AUTORAL


USER FUNCTION CD010ACONT()
LOCAL CPERIOD := AH1->AH1_PERIOD
LOCAL DDTAULTP := CTOD("  /  /    ")

DDTAULTP := U_GCDAH1A(CPERIOD)

IF EMPTY(AH1->AH1_DTULTP) .OR. AH1->AH1_DTULTP <> DDTAULTP
	RECLOCK("AH1",.F.)
	AH1->AH1_DTULTP := DDTAULTP
	AH1->(MSUNLOCK())
ENDIF


RETURN()


